@{
    Layout = null;
}
@model  Poco.Feedback
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetInfo</title>
    <style>
        .tableList
{
    border: 1px solid #E7E7E7;
    margin: 10px auto 15px auto;
}
.tableList th
{
    height: 30px;
    border-left: 1px solid #E7E7E7;
    font-weight: normal;
}
.tableList th:first-child
{
    border-left: none;
}
.tableList td
{
    border-left: 1px solid #E7E7E7;
    border-top: 1px solid #E7E7E7;
    height: 40px;
    padding: 2px;
    font-size: 15px;
}
.tableList td:first-child
{
    border-left: none;
}
    </style>
</head>
<body>
    <div>
        <table class="tableList" cellpadding="0" cellspacing="0" style=" width:400px; line-height:25px">
            <tr>
                <td style=" width:100px">客户端：</td>
                <td  style=" width:300px">@Model.client</td>
            </tr>
             <tr>
                <td>客户：</td>
                <td>@Model.AccountMain.Name</td>
            </tr>
             <tr>
                <td>提交日期：</td>
                <td>@Model.CreateDate</td>
            </tr>
             <tr>
                <td valign="top">内容：</td>
                <td>@Model.Content</td>
            </tr>
             <tr>
                <td>联系方式：</td>
                <td>@Model.contact</td>
            </tr>
        </table>
    </div>
</body>
</html>
