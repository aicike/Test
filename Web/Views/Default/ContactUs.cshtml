@{
    ViewBag.Title = "ContactUs";
    Layout = "~/Views/Shared/_LayoutDefault.cshtml";
}
@section head{
    <style>
        /* vertical tabs */
        .tabs
        {
            padding: 0px;
            width: 160px;
        }
        #vertical-tabs ul.tabs li.current, #vertical-tabs ul.tabs li:hover
        {
            background: #00aec8;
        }
        
        .tabscontent.color
        {
            background-color: #00aec8 !important;
        }
        
        #vertical-tabs .tabscontent
        {
            border-left: 1px solid #00aec8;
            min-height: 400px;
        }
        
        #vertical-tabs
        {
            margin-bottom: 15px;
        }
        
        #vertical-tabs .contents
        {
            float: right;
            width: 800px;
        }
        
        #vertical-tabs ul.tabs
        {
            overflow: hidden;
            float: left;
        }
        
        #vertical-tabs ul.tabs li
        {
            display: inline-block;
            padding: 12px 0px 12px 10px;
            margin-bottom: 4px;
            cursor: pointer;
            width: 100%;
        }
        
        #vertical-tabs .tabscontent
        {
            padding: 15px;
            display: none;
            border-left: 1px solid #00aec8;
        }
        
        .p1
        {
            margin: 15px auto 15px auto;
        }
        .p2
        {
            margin: 15px auto 0px auto;
        }
        .p3
        {
            margin: 10px auto 10px auto;
        }
    </style>
    <script src="@Url.Content("~/Scripts/tytabs.js")" type="text/javascript"></script>
    <script language="javascript" type="text/javascript">

        $(function () {
            $("#vertical-tabs").tytabs({
                prefixtabs: "tabz",
                prefixcontent: "contentz"
            });
            $("#vertical-tabs.a").tytabs({
                prefixtabs: "taba",
                prefixcontent: "contenta"
            });
            $("#vertical-tabs.b").tytabs({
                prefixtabs: "tabb",
                prefixcontent: "contentb"
            });
            $("#vertical-tabs.c").tytabs({
                prefixtabs: "tabc",
                prefixcontent: "contentc"
            });

            var reg = new RegExp("(^|&)" + "v" + "=([^&]*)(&|$)", "i");
            var r = window.location.search.substr(1).match(reg);
            var type = "";
            if (r != null) {
                type = unescape(r[2]);
            }
            switch (type) {
                case "aboutus":
                    $("#tabz2").click();
                    break;
                case "agreement":
                    $("#tabz3").click();
                    break;
                default:
                    break;
            }
        });
    </script>
}
